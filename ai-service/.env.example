# Server Configuration
PYTHON_ENV=development
API_PORT=8000
API_HOST=0.0.0.0

# OCR Configuration
OCR_ENGINE=easyocr
OCR_CONFIDENCE_THRESHOLD=0.8
OCR_LANGUAGES=en
TESSERACT_CMD_PATH=tesseract

# Image Processing Configuration
MAX_IMAGE_SIZE=2048
IMAGE_QUALITY=95
PREPROCESSING_ENABLED=true
SUPER_RESOLUTION_ENABLED=false

# Text Similarity Configuration
SIMILARITY_THRESHOLD=0.9
SIMILARITY_METHOD=rapidfuzz
FUZZY_MATCH_THRESHOLD=85

# Model Configuration
MODEL_PATH=./models
ENABLE_GPU=false
BATCH_SIZE=1
MAX_WORKERS=4

# Logo Detection Configuration
LOGO_DETECTION_ENABLED=false
LOGO_CONFIDENCE_THRESHOLD=0.85
LOGO_MODEL_PATH=./models/logo_detector.pth

# Anomaly Detection Configuration
ANOMALY_DETECTION_ENABLED=false
ANOMALY_THRESHOLD=0.15
ANOMALY_MODEL_PATH=./models/anomaly_detector.pth

# File Management
TEMP_PATH=./temp
CLEANUP_INTERVAL=3600
MAX_TEMP_FILES=1000

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/ai_service.log

# Performance Configuration
CACHE_ENABLED=true
CACHE_TTL=3600
REQUEST_TIMEOUT=30

# Debug Configuration
DEBUG_MODE=false
SAVE_INTERMEDIATE_IMAGES=false
DEBUG_OUTPUT_PATH=./debug